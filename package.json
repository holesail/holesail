{
  "name": "holesail",
  "version": "2.3.2",
  "description": "Holesail is TCP/UDP peer-to-peer proxy. Holesail let's you instantly share any application running on a specific port from your local computer.",
  "main": "src/index.js",
  "pear": {
    "name": "holesail",
    "type": "terminal",
    "stage": {
      "ignore": [
        ".github",
        ".gitignore",
        ".idea",
        ".vscode"
      ]
    }
  },
  "imports": {
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    }
  },
  "scripts": {
    "test": "standard && node test.mjs",
    "refresh": "rm package-lock.json && rm -rf node_modules && npm i && npm update",
    "build:prepare": "npx bare-make generate --platform darwin --arch arm64",
    "build": "npm run build:prepare && npx bare-make build && npm run build:strip",
    "build:strip": "npx bare-make install --strip",
    "lint-check": "npx prettier . --check",
    "lint": "npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holesail/holesail.git"
  },
  "keywords": [
    "holesail",
    "hyperdht",
    "holepunch"
  ],
  "author": "supersuryaansh",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/holesail/holesail/issues"
  },
  "homepage": "https://github.com/holesail/holesail#readme",
  "pkg": {
    "assets": [
      "includes/**/*"
    ]
  },
  "overrides": {
    "sodium-native": "5.0.8"
  },
  "dependencies": {
    "bare-crypto": "^1.12.0",
    "bare-module": "^6.1.2",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.2",
    "bare-querystring": "^1.0.0",
    "barely-colours": "^1.0.0",
    "child_process": "npm:bare-subprocess@^5.1.5",
    "cli-box": "6.0.10",
    "colors": "^1.4.0",
    "graceful-goodbye": "^1.3.3",
    "holesail-client": "^2.3.0",
    "holesail-logger": "^1.1.0",
    "holesail-server": "^2.4.0",
    "hyper-cmd-lib-keys": "^0.1.1",
    "livefiles": "^1.1.0",
    "minimist": "^1.2.8",
    "module": "npm:bare-node-module@^1.0.0",
    "path": "npm:bare-node-path@^1.0.1",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^2.0.0",
    "process": "npm:bare-process@^4.2.1",
    "qrcode-terminal": "^0.12.0",
    "querystring": "npm:bare-node-querystring@^1.0.0",
    "ready-resource": "^1.2.0",
    "url": "npm:bare-url@^2.3.2",
    "which-runtime": "^1.3.2",
    "z32": "^1.1.0"
  },
  "bin": {
    "holesail": "./src/bin/holesail.mjs"
  },
  "devDependencies": {
    "bare-make": "^1.6.3",
    "cmake-bare-bundle": "^2.1.4",
    "cmake-fetch": "^1.4.6"
  }
}
